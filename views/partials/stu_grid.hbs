{{#each students}}
<div class="col-xs-5 col-sm-4 col-md-3 col-lg-2">
    <div class="" data-toggle="modal" data-target="#myModal">
        <a class="thumbnail pg-thumb" onclick="changeModal('{{ucid}}')">
            <img src="https://w3.ucollege.edu/PG/GetImage.ashx?Id=P000{{ucid}}" alt="{{ucid}}" class="img-rounded img-responsive">
            <div class="caption">
                <h3>{{nameFirst}}</h3>
                <h3>{{nameLast}}</h3>
                <p>{{classStanding}}</p>
                <p>{{major}}</p>
            </div>
        </a>
    </div>
</div>
{{/each}}

{{> stu_modal}}
